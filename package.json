{
  "name": "not-validation",
  "version": "0.0.1",
  "description": "not-node validation framework",
  "main": "src/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require mocha-suppress-logs --reporter spec  --timeout 12000",
    "lint": "./node_modules/.bin/eslint ./src --fix",
    "pretest": "./node_modules/.bin/eslint ./src",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "cover": "nyc npm test"
  },
  "engines": {
    "node": ">12.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interrupter/not-validation.git"
  },
  "keywords": [
    "validation",
    "node"
  ],
  "author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/interrupter/not-validation/issues"
  },
  "dependencies": {
    "not-error": "^0.2.1"
  },
  "devDependencies": {
    "chai": "*",
    "chai-as-promised": "*",
    "eslint": "^8.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.7",
    "mocha": "*",
    "mocha-suppress-logs": "^0.3.1",
    "nyc": "^15.1.0",
    "retire": "^3.0.3"
  },
  "homepage": "https://github.com/interrupter/not-validation#readme",
  "nyc": {
    "include": [
      "index.js",
      "src/**/**/**.js"
    ],
    "exclude": [
      "tests/**/**/**/**.js"
    ],
    "reporter": [
      "html"
    ],
    "check-coverage": false,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  }
}
